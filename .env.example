# Database Connection
DB_HOST=your-postgres-host.com
DB_PORT=5432
DB_DATABASE=defaultdb
DB_USERNAME=your_username
DB_PASSWORD=your_password
DB_SSLMODE=require

# SSL Certificate (only needed for some managed PostgreSQL services)
# Uncomment and set certificate filename if your database requires SSL cert
# DB_SSLROOTCERT=ca-certificate.crt
# DB_SSLROOTCERT=your-certificate.pem

# API Configuration
ANALYTICS_TOKEN=your-analytics-token
PORT=3000
NODE_ENV=development

# Mixpanel Export Credentials (for migration)
MIXPANEL_USERNAME=your-mixpanel-service-account
MIXPANEL_PASSWORD=your-mixpanel-password
MIXPANEL_PROJECT_ID=your-project-id